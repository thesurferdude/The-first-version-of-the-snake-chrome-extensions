{
    "manifest_version": 2,
    "name": "Snake!",
    "version": "1.2",
    "content_scripts": [
        
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["snake.js"]
        }
    ],
    "background":{
        "scripts":["background.js"]
    },
    "browser_action": {
        "default_popup": "index.html"

    }
}